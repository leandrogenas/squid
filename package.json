{
  "name": "squid",
  "productName": "Squid Tasker",
  "description": "Scraper de conte√∫dos",
  "version": "0.1.20210509",
  "license": "UNLICENSED",
  "main": "target/index.js",
  "author": {
    "email": "leandro.fabri96@gmail.com",
    "name": "Leandro Fabri Pereira",
    "url": "https://leandragem.dev"
  },
  "engines": {
    "node": "^14.15"
  },
  "browser": {
    "fs": true,
    "path": true,
    "os": true
  },
  "scripts": {
    "clean": "rimraf dist main renderer/out renderer/.next",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "dev": "npm run build-electron && electron .",
    "build-renderer": "next build renderer && next export renderer -o target/renderer",
    "build-electron": "tsc -p electron-src",
    "build": "npm run build-renderer && npm run build-electron",
    "pack-app": "npm run build && electron-builder",
    "dist": "npm run build && electron-builder --dir",
    "tst": "electron-builder --dir",
    "type-check": "tsc",
    "postinstall": "node ./postinstall.js",
    "tst:start:electron": "electron .",
    "tst:build": "npm-run-all tst:build:electron tst:build:react",
    "tst:build:run": "npm-run-all tst:build tst:start:electron",
    "tst:build:electron": "webpack --config webpack/electron.webpack.js",
    "tst:build:react": "webpack --config webpack/react.webpack.js",
    "tst:package": "npm-run-all build package:dist",
    "tst:package:dist": "electron-builder --dir",
    "package:portable": "npm run build && electron-builder --dir --win --portable"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.44.2",
    "@blueprintjs/table": "^3.8.30",
    "@reduxjs/toolkit": "^1.5.1",
    "@types/es6-shim": "^0.31.41",
    "axios": "^0.21.1",
    "css-select": "^4.1.2",
    "dexie": "^3.0.3",
    "electron-chromedriver": "^13.0.0",
    "electron-download-manager": "^2.1.2",
    "electron-is-dev": "^1.1.0",
    "electron-next": "^3.1.5",
    "html-react-parser": "^1.2.6",
    "next": "^10.2.0",
    "next-page-transitions": "1.0.0-beta.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-skeleton-css": "^1.1.0",
    "redux": "^4.1.0",
    "redux-devtools-extension": "^2.13.9",
    "selenium-webdriver": "^4.0.0-beta.4",
    "styled-components": "^5.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@testing-library/jest-dom": "^5.0.0",
    "@testing-library/react": "^11.0.0",
    "@testing-library/user-event": "^13.0.0",
    "@types/axios": "^0.14.0",
    "@types/dexie": "^1.3.1",
    "@types/electron": "^1.6.10",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.2",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "@types/react-icons": "^3.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/selenium-webdriver": "^4.0.13",
    "@types/styled-components": "^5.1.10",
    "@types/uuid": "^8.3.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "commitizen": "^4.1.2",
    "css-loader": "^5.2.4",
    "electron": "^10.0.0",
    "electron-builder": "^22.11.1",
    "electron-reload": "^1.5.0",
    "file-loader": "^6.2.0",
    "husky": "^4.2.5",
    "jest": "^26.6.3",
    "jest-css-modules-transform": "^4.2.0",
    "next-compose-plugins": "^2.2.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "build": {
    "appId": "dev.leandragem.w.squid",
    "productName": "Squid",
    "files": [
      "megacmd/*",
      "target/**"
    ],
    "asarUnpack": [
      "../*"
    ],
    "linux": {
      "target": [
        "deb",
        "snap",
        "rpm"
      ],
      "category": "Utility"
    },
    "deb": {},
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "public/icons/squid.ico"
    },
    "nsis": {
      "installerIcon": "public/icons/squid.ico",
      "uninstallerIcon": "public/icons/squid.ico",
      "uninstallDisplayName": "Squid Uninstaller",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
